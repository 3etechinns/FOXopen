  <xs:attribute name="fieldHeight" type="xs:unsignedInt"/>
  <xs:attribute name="formColChars" type="xs:positiveInteger">
    <xs:annotation>
      <xs:documentation>Number of characters allowed per form layout display grid column. Allowed against Collection Type schema elements.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="formMaxCols" type="xs:positiveInteger">
    <xs:annotation>
      <xs:documentation>Number of columns allowed in form layout display grid. Allowed against Collection Type schema elements.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="emptyText"/>
  <xs:attribute name="fieldxlock" type="xs:positiveInteger"/>
  <xs:attribute name="fieldylock">
    <xs:simpleType>
      <xs:restriction base="xs:positiveInteger"/>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="displayOrder" type="fox-xpath-type">
    <xs:annotation>
      <xs:documentation>Number or XPath string of number to use when comparing to other elements with a displayOrder to define the order elements are displayed in</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="displayBefore" type="fox-xpath-type">
    <xs:annotation>
      <xs:documentation>String or XPath string of sibling element name for the element to display before</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="displayAfter" type="fox-xpath-type">
    <xs:annotation>
      <xs:documentation>String or XPath string of sibling element name for the element to display after</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="visitOrder" type="xs:integer"/>
  <xs:attribute name="validate-map-set" type="yn-flag">
    <xs:annotation>
      <xs:documentation>This is only relevent when map-set specified, and ensures selected (or entered) values match values in the map-set. Default is Y.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="validate-xpath" type="fox-xpath-type">
    <xs:annotation>
      <xs:documentation>An XPATH expression which is evaluated as boolean(XPATH) , and must return true for validation to be successful.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="validate-xpath-msg">
    <xs:annotation>
      <xs:documentation>The prefix error text used when validate-xpath returns false. Default is "Invalid {prompt}".</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="validate-map-set-msg">
    <xs:annotation>
      <xs:documentation>The prefix error text used when map-set validation violation occurs. Default is "Invalid {prompt}".</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="validate-general-msg">
    <xs:annotation>
      <xs:documentation>The prefix error text used for general validation violation occurs. "validate-map-set-msg" and "validate-xpath-msg" are used in preference for those types of error. Overrides system generated default of "Invalid {prompt}". </xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="validate-when">
    <xs:annotation>
      <xs:documentation>XPath boolean. If true, the node will be validated by XPath commands. Default is true if unspecified.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <!-- APPLICATION / MODULE / STATE DISPLAY ATTRIBUTES-->
  <xs:attribute name="fieldMaxHeight"/>
