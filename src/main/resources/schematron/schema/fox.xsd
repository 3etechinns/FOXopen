    <xs:annotation>
      <xs:documentation>Add this string to the style attribute to the containing element of the field for this element</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="formCellClass" type="fox-optional-xpath-string">
    <xs:annotation>
      <xs:documentation>Add this string to the class attribute to the containing element of the prompt/field for this element only when set out in a form</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="formCellStyle" type="fox-optional-xpath-string">
    <xs:annotation>
      <xs:documentation>Add this string to the style attribute to the containing element of the prompt/field for this element only when set out in a form</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="listCellClass" type="fox-optional-xpath-string">
    <xs:annotation>
      <xs:documentation>Add this string to the class attribute to the containing element of the prompt/field for this element only when set out in a list</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="listCellStyle" type="fox-optional-xpath-string">
    <xs:annotation>
      <xs:documentation>Add this string to the style attribute to the containing element of the prompt/field for this element only when set out in a list</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="mandatoryClass" type="fox-optional-xpath-string">
    <xs:annotation>
      <xs:documentation>Add this string to the class attribute to the containing element of the mandatory/optional text</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="mandatoryStyle" type="fox-optional-xpath-string">
    <xs:annotation>
      <xs:documentation>Add this string to the style attribute to the containing element of the mandatory/optional text</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="keyClass" type="fox-optional-xpath-string">
    <xs:annotation>
      <xs:documentation>Add this string to the class attribute to the containing element of the mapset key text</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="keyStyle" type="fox-optional-xpath-string">
    <xs:annotation>
      <xs:documentation>Add this string to the style attribute to the containing element of the mapset key text</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="actionClass" type="fox-optional-xpath-string">
    <xs:annotation>
      <xs:documentation>Add this string to the class attribute to the field if the widget type is an Action type</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="actionStyle" type="fox-optional-xpath-string">
    <xs:annotation>
      <xs:documentation>Add this string to the style attribute to the field if the widget type is an Action type</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="href" type="fox-xpath-string">
    <xs:annotation>
      <xs:documentation>Set the location a URL widget should take you to</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="email" type="fox-xpath-string">
    <xs:annotation>
      <xs:documentation>Pre-define an email recipient to go into the mailto link when using the mailto widget</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="subject" type="fox-xpath-string">
    <xs:annotation>
      <xs:documentation>Pre-define a subject to go into the mailto link when using the mailto widget</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="cc" type="fox-xpath-string">
    <xs:annotation>
      <xs:documentation>Pre-define a CSV List of email addresses when using the mailto widget</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="clean">
    <xs:annotation>
      <xs:documentation>Data can have whitespace trimmed, or not, when going into/out of the DOM</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="entered-string">
        <xs:enumeration value="trim"/>
        <xs:enumeration value="no-trim"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="case">
    <xs:annotation>
      <xs:documentation>Data can have its case changed when going into/out of the DOM</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="entered-string">
        <xs:enumeration value="upper"/>
        <xs:enumeration value="lower"/>
        <xs:enumeration value="initCap"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="formatDate" type="xs:string">
    <xs:annotation>
      <xs:documentation>Data can have a custom date format applied (or default if this attribute is empty), when going into/out of the DOM</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="inputMask">
    <xs:annotation>
      <xs:documentation>Data can have commas added if it's marked as an xs:Decimal type, when going into/out of the DOM</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="entered-string">
        <xs:enumeration value="commas"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="client-visibility-rule" type="xs:string"/>
  <xs:attribute name="client-visibility-rule-value" type="xs:string"/>
  <xs:attribute name="client-visibility-rule-target" type="xs:string"/>
  <xs:attribute name="spatial-definition" type="xs:string"/>
  <xs:attribute name="spatial-interface" type="xs:string"/>
  <xs:attribute name="canvas-callback-action" type="xs:string"/>
  <xs:attribute name="canvas-cache-key" type="xs:string"/>
  <xs:attribute name="htmlWidgetConfig">
    <xs:annotation>
      <xs:documentation>HTML Widget configuration</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="standard"/>
        <xs:enumeration value="expanded"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="layoutMethod" type="fox-xpath-type">
    <xs:annotation>
      <xs:documentation>Layout method to use when doing a form set-out. Currently there is 'form' and 'test'.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="formList" type="fox-xpath-boolean">
    <xs:annotation>
      <xs:documentation>If this is set to true lists will set out a list of form set-outs for each row instead of displaying the data in a table</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="collapseColumns" type="fox-xpath-type">
    <xs:annotation>
      <xs:documentation>XPath boolean to determine a list set-out should hide empty columns, where empty means there's no value in the dom for any row or hasContent is set to false.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="hasContent" type="fox-xpath-type">
    <xs:annotation>
      <xs:documentation>XPath boolean to determine if this column should be shown when collapseColumns is enabled.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="mandatoryText" type="entered-string">
    <xs:annotation>
      <xs:documentation>Text to show when an element is mand/mandatory and mandatoryDisplay is "both" or "mandatory". Default is "*"</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="optionalText" type="entered-string">
    <xs:annotation>
      <xs:documentation>Text to show when an element is not mand/mandatory and mandatoryDisplay is "both" or "optional"</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="mandatoryDisplay">
    <xs:annotation>
      <xs:documentation>Should the set-out display mandatoryText, optionalText, both or none next to elements as appropriate. Default is "mandatory"</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="mandatory"/>
        <xs:enumeration value="optional"/>
        <xs:enumeration value="both"/>
        <xs:enumeration value="none"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="phantom-data-xpath" type="fox-xpath-dom">
    <xs:annotation>
      <xs:documentation>XPath to another element to set out (read only mode only) in place of this phantom element</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="phantom-menu-mode" type="fox-xpath-boolean">
    <xs:annotation>
      <xs:documentation>XPath to decide if the namespace the attribute is under is used for a phantom-menu.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="phantom-buffer" type="fox-xpath-string">
    <xs:annotation>
      <xs:documentation>Displays a named buffer in context of the phantom element.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="phantom-buffer-attach" type="fox-xpath-dom">
    <xs:annotation>
      <xs:documentation>Attach point to use when evaluating the phantom-buffer.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="emptyListBuffer" type="fox-xpath-string">
    <xs:annotation>
      <xs:documentation>Name of a buffer to display in place of a list when the list has 0 rows inside it.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="emptyListBufferAttach" type="fox-xpath-dom">
    <xs:annotation>
      <xs:documentation>Attach point to use when evaluating the empty list buffer.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="enableListsInForms" type="xs:boolean" default="false">
    <xs:annotation>
      <xs:documentation>Show lists that are nested under forms. FOX4 didn't allow this but FOX5 can.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="enableFocusHintDisplay" type="xs:boolean" default="false">
    <xs:annotation>
      <xs:documentation>Enable showing of hints when fields get focus instead of only when using the mouse to hover the hint icon</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="namespacePreference" type="fox-xpath-type">
    <xs:annotation>
      <xs:documentation>Which namespaces, if enabled, should be checked first when looking for attributes. This attribute should be prefixed with the fox namespace only</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="cellmateKey" type="entered-string">
    <xs:annotation>
      <xs:documentation>You can set this to '.' if you want a key to be auto-generated or manually set a key to display to aid in debugging issues with cellmates, can be used to have multiple cellmate sequences defined separately aggregate into one wherever the first one is defined</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="downloadAsAttachment" type="fox-xpath-type">
    <xs:annotation>
      <xs:documentation>Controls whether the download from a file widget is served as an attachment (i.e. user is prompted with a 'Save As...' dialog) or not (i.e. the browser will attempt to open the file directly). Default is true.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="downloadFileName" type="entered-string">
    <xs:annotation>
      <xs:documentation>XPath to determine a filename to be appended to a download link URL when actionType = "download". This is used to help some browsers determine the name of a downloaded file.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="reverseBooleanOrder" type="fox-xpath-boolean">
    <xs:annotation>
      <xs:documentation>Controls whether the true/false values should be reversed for display of a xs:boolean field - i.e. false before true (the default is always true before false). Must be an XPath that resolves to true (i.e. "true()" or ".").</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="descriptionDisplayMode" type="display-mode-enum" default="always">
    <xs:annotation>
      <xs:documentation>Control when to show the description based on the element's visibility level (ro/edit)</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="hintDisplayMode" type="display-mode-enum" default="always">
    <xs:annotation>
      <xs:documentation>Control when to show the hint based on the element's visibility level (ro/edit)</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="mandatoryDisplayMode" type="display-mode-enum" default="edit">
    <xs:annotation>
      <xs:documentation>Control when to show the mandatory display based on the element's visibility level (ro/edit)</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="default-item-widget"/>
  <xs:attribute name="default-selector-widget"/>
  <xs:attribute name="default-date-widget"/>
  <xs:attribute name="default-widget"/>
  <xs:attribute name="upload-mode">
    <xs:annotation>
      <xs:documentation>The upload mode for a file upload. Interruption refers to whether the base page sends a signal to the upload window to terminate the upload in the event of a module push or pop. Interruption can only be disabled when the upload target is a DOM in a storage location (i.e. root).</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="interrupt-on-module-push-pop-upload-target-any"/>
        <xs:enumeration value="no-interrupt-upload-target-storage-location"/>
        <xs:enumeration value="no-interrupt-modal-upload-window"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="upload-metadata">
    <xs:annotation>
      <xs:documentation>Choose the type of metadata inserted into the DOM on completion of an upload. 'Basic' stores metadata about the last successful upload. 'Diagnostic' additionally provides information about the latest upload attempt, regardless of its status. Default is basic.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="basic"/>
        <xs:enumeration value="diagnostic"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="upload-success-action" type="xs:string"/>
  <xs:attribute name="upload-fail-action" type="xs:string"/>
  <xs:attribute name="upload-complete-action" type="xs:string"/>
  <xs:attribute name="upload-file-type" type="xs:string"/>
  <xs:attribute name="upload-enable-download" type="xs:string"/>
  <xs:attribute name="upload-enable-view" type="xs:string"/>
  <xs:attribute name="upload-enable-description" type="xs:string"/>
  <xs:attribute name="upload-window-type">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="modal"/>
        <xs:enumeration value="modeless"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="upload-prompt"/>
  <xs:attribute name="upload-view-prompt"/>
  <xs:attribute name="upload-choose-prompt"/>
  <xs:attribute name="upload-save-prompt"/>
  <xs:attribute name="upload-field-flow" default="horizontal">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="horizontal"/>
        <xs:enumeration value="vertical"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="upload-button-flow" default="horizontal">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="horizontal"/>
        <xs:enumeration value="vertical"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="upload-button-position" default="south">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="north"/>
        <xs:enumeration value="south"/>
        <xs:enumeration value="east"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="upload-widget-style" type="xs:string"/>
  <xs:attribute name="uploadWholePageDropZone" type="entered-string"/>
  <xs:attribute name="uploadMaxFiles" type="entered-string"/>
  <xs:attribute name="image-only" type="fox-xpath-type"/>
  <xs:attribute name="image-enable-preview" type="fox-xpath-type"/>
  <xs:attribute name="image-enable-rotate" type="fox-xpath-type"/>
  <xs:attribute name="image-display-size" type="fox-xpath-type"/>
  <xs:attribute name="image-display-width" type="fox-xpath-type"/>
  <xs:attribute name="image-display-height" type="fox-xpath-type"/>
  <xs:attribute name="image"/>
  <xs:attribute name="image-xpath"/>
  <xs:attribute name="imageUrl"/>
  <xs:attribute name="file-storage-location">
    <xs:annotation>
      <xs:documentation>A storage location for any content that can be stored in a file.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:annotation>
        <xs:documentation>The storage location where the file resides.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string"/>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="compare-id">
    <xs:annotation>
      <xs:documentation>Used with the fm:compare command</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="renderType" default="form">
    <xs:annotation>
      <xs:documentation>Controls how the field widget is displayed, for example whether tickboxes should appear ('form') or only the tickbox text should appear ('print').</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="entered-string">
        <xs:enumeration value="form"/>
        <xs:enumeration value="print"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="fieldPrefix">
    <xs:annotation>
      <xs:documentation>Text to display read-only before the field. Only show for single-line input/input+ widgets</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="fieldSuffix">
    <xs:annotation>
      <xs:documentation>Text to display read-only after the field. Only show for single-line input/input+ widgets</xs:documentation>
    </xs:annotation>
  </xs:attribute>
</xs:schema>
