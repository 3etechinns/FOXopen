  <xs:attribute name="ro" type="fox-xpath-boolean"/>
  <!-- SCHEMA ELEMENT DISPLAY ATTRIBUTES-->
  <xs:attribute name="widget">
    <xs:annotation>
      <xs:documentation>The presentation widget</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="button"/>
        <xs:enumeration value="button+"/>
        <xs:enumeration value="captcha"/>
        <xs:enumeration value="cartographic"/>
        <xs:enumeration value="date"/>
        <xs:enumeration value="date+"/>
        <xs:enumeration value="datetime"/>
        <xs:enumeration value="datetime+"/>
        <xs:enumeration value="file"/>
        <xs:enumeration value="file-new"/>
        <xs:enumeration value="file-upload"/>
        <xs:enumeration value="html"/>
        <xs:enumeration value="html+"/>
        <xs:enumeration value="image"/>
        <xs:enumeration value="input"/>
        <xs:enumeration value="input+"/>
        <xs:enumeration value="link"/>
        <xs:enumeration value="link+"/>
        <xs:enumeration value="mailto"/>
        <xs:enumeration value="password"/>
        <xs:enumeration value="print"/>
        <xs:enumeration value="radio"/>
        <xs:enumeration value="selector"/>
        <xs:enumeration value="selector+"/>
        <xs:enumeration value="search-selector"/>
        <xs:enumeration value="static-text"/>
        <xs:enumeration value="tickbox"/>
        <xs:enumeration value="timer+"/>
        <xs:enumeration value="url"/>
        <xs:enumeration value="url-btn"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="action" type="entered-string">
    <xs:annotation>
      <xs:documentation>The name of an action to run if the field is runnable</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="action-context" type="fox-xpath-dom">
    <xs:annotation>
      <xs:documentation>Sets :{action} to the node specified by this XPath</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="defaultAction" type="fox-xpath-boolean">
    <xs:annotation>
      <xs:documentation>Indicates the action is the default for the form and should be run if the Enter key is pressed.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="change-action" type="entered-string">
    <xs:annotation>
      <xs:documentation>Fires an action when a piece of data changes</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="confirm" type="fox-xpath-string">
    <xs:annotation>
      <xs:documentation>Message displayed in a browser confirm dialog box before an action is run</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="actionType">
    <xs:annotation>
      <xs:documentation>Controls how the action is invoked by the user. "Churn" results in a form post. "Download" displays a download link directly to a file. The invoked action must result in a download being generated.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="entered-string">
        <xs:enumeration value="churn"/>
        <xs:enumeration value="download"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="emptyText" type="fox-xpath-string">
    <xs:annotation>
      <xs:documentation>Text to show when this field is set out as a readonly text field with no text content</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="placeholder" type="fox-xpath-string">
    <xs:annotation>
      <xs:documentation>Text to put as placeholder in the input</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="map-set" type="entered-string">
    <xs:annotation>
      <xs:documentation>The name of a map-set definition to convert key/value data with and populate selector/tickbox/radio options</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="map-set-attach" type="fox-xpath-dom">
    <xs:annotation>
      <xs:documentation>Sets the context :{map-set-attach} to the XPath specified by this attribute.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="selector" type="fox-xpath-dom">
    <xs:annotation>
      <xs:documentation>XPath targeting one nested node which specifies cardinality generator for multi-select map-set widgets.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="radio-owner" type="fox-xpath-dom">
    <xs:annotation>
      <xs:documentation>XPATH expression that locates an element that when common with another radio item, and the same radio-group label is in use, the item joins the logical radio set. The default is "." indicating the current record. This allows radio groups to be defined across records (i.e. within a common parent or super-parent list e.g. ".."  See also radio-group.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="radio-group" type="entered-string">
    <xs:annotation>
      <xs:documentation>Label string that associates distinct items to form a logical radio group. This default value is the absolute path of the item in the schema definition. This attribute allows radio groups to be defined across items. When used with radio-owner, across records can also be acheived. See also radio-owner.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="key-true" type="fox-optional-xpath-string">
    <xs:annotation>
      <xs:documentation>The display key for true in boolean selector widgets. Default to Yes</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="key-false" type="fox-optional-xpath-string">
    <xs:annotation>
      <xs:documentation>The display key for false in boolean selector widgets. Defaults to No.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="key-null" type="fox-optional-xpath-string">
    <xs:annotation>
      <xs:documentation>The display key for null content selector widgets. Defaults to None. This is only used for optional elements. Mand elements use key-missing.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="key-missing" type="fox-optional-xpath-string">
    <xs:annotation>
      <xs:documentation>The display key for null content selector widgets. Defaults to Select One|Many. This is only used for mand elements. Optinal elements use key-null.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="key-unrecognised" type="fox-optional-xpath-string">
    <xs:annotation>
      <xs:documentation>The display key for unrecognised content selector widgets. Defaults to content string (if simple) otherwise Unrecognised.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="suppressUnselected" type="fox-xpath-boolean">
    <xs:annotation>
      <xs:documentation>XPath boolean. If true, multi-select widgets will not show unselected options when in read only mode. Default is false.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="searchDisplayHierarchy" type="fox-xpath-boolean">
    <xs:annotation>
      <xs:documentation>XPath bool whether to show indented suggestion lists</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="searchIndentMultiplier" type="fox-xpath-number">
    <xs:annotation>
      <xs:documentation>How much to indent each item by in the hierarchical suggestion list</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="searchSuggestionWidth" type="fox-xpath-number">
    <xs:annotation>
      <xs:documentation>Hard width for the suggestions list specified just as number, engine turns to em</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="searchSuggestionMaxWidth" type="fox-xpath-number">
    <xs:annotation>
      <xs:documentation>Max width for the suggestions list specified just as number, engine turns to em</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="searchSuggestionMaxHeight" type="fox-xpath-number">
    <xs:annotation>
      <xs:documentation>Max height for the suggestions list specified just as number, engine turns to em</xs:documentation>
    </xs:annotation>
  </xs:attribute>
